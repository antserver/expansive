#   mbedtls.obj
#
DEPS_7 = $(DEPS_7) build\$(CONFIG)\inc\mpr.h

build\$(CONFIG)\obj\mbedtls.obj: \
    src\mpr-mbedtls\mbedtls.c $(DEPS_7)
	@echo .. [Compile] build\$(CONFIG)\obj\mbedtls.obj
	"$(CC)" -c -Fo$(BUILD)\obj\mbedtls.obj -Fd$(BUILD)\obj\mbedtls.pdb $(CFLAGS) -D_FILE_OFFSET_BITS=64 $(DFLAGS) $(IFLAGS) src\mpr-mbedtls\mbedtls.c $(LOG)

!IF "$(ME_COM_SSL)" == "1"
!IF "$(ME_COM_MBEDTLS)" == "1"
#
#   mbedtls
#
!IF "$(ME_COM_MBEDTLS)" == "1"
DEPS_8 = $(DEPS_8) build\$(CONFIG)\bin\libmbedtls.lib
!ENDIF
DEPS_8 = $(DEPS_8) build\$(CONFIG)\obj\mbedtls.obj

build\$(CONFIG)\bin\libmpr-mbedtls.lib: $(DEPS_8)
	@echo ..... [Link] build\$(CONFIG)\bin\libmpr-mbedtls.lib
	"lib.exe" -nologo -out:$(BUILD)\bin\libmpr-mbedtls.lib "$(BUILD)\obj\mbedtls.obj" $(LOG)
