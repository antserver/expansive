#
DEPS_19 = $(DEPS_19) src\zlib\zlib.h
DEPS_19 = $(DEPS_19) build\$(CONFIG)\inc\me.h

build\$(CONFIG)\inc\zlib.h: $(DEPS_19)
	@echo ..... [Copy] build\$(CONFIG)\inc\zlib.h
